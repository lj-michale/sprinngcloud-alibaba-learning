<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.turing.springboot.dao.mapper.UpdateTimeLineMapper" >

    <select id="getUpdateTimeLines"  resultType="com.turing.springboot.dao.entity.UpdateTimeLine">
        select r.id,
               r.author,
               r.action,
               r.context,
               r.create_time,
               r.create_by
        from bigdata.update_time_line r
    </select>

</mapper>